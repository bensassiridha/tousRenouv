RewriteRule ^(.*)/AjaxVille2.php$ zone/AjaxVille2.php
RewriteRule ^(.*)/chat.php$ zone/chat.php
RewriteRule ^(.*)/annonces.php$ zone/annonces.php
RewriteRule ^(.*)/panier.php$ zone/panier.php
RewriteRule ^(.*)/espace_affilier.php$ zone/espace_affilier.php
RewriteRule ^(.*)/espace_particulier.php$ zone/espace_particulier.php
RewriteRule ^(.*)/espace_professionnel.php$ zone/espace_professionnel.php
RewriteRule ^(.*)/espace_pro.php$ zone/espace_pro.php
RewriteRule ^(.*)/index.php$ zone/index.php
RewriteRule ^(.*)/services.php$ zone/services.php
RewriteRule ^(.*)/activites.php$ zone/activites.php
RewriteRule ^(.*)/recherche.php$ zone/recherche.php
RewriteRule ^(.*)/conseils.php$ zone/conseils.php
RewriteRule ^(.*)/simulateur.php$ zone/simulateur.php
RewriteRule ^(.*)/propos.php$ zone/propos.php
RewriteRule ^(.*)/contact.php$ zone/contact.php
