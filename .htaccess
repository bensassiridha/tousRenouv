
Options +FollowSymlinks
RewriteEngine on


RewriteCond %{HTTP_HOST} !^tousrenov\.dev [NC]
RewriteRule (.*) .tousrenov.dev/$1 [QSA,R=301,L]

RewriteRule ^(.*)/auth.php$ espace_particulier/espace_particulier.php

RewriteRule ^ttre_adm/$ ttre_adm/ [L]
RewriteRule ^ttre_adm/AjaxConnectAdmin.php$ ttre_adm/AjaxConnectAdmin.php [L]
RewriteRule ^ttre_adm/AjaxFichChat.php$ ttre_adm/AjaxFichChat.php [L]
RewriteRule ^ttre_adm/AjaxVerifChat.php$ ttre_adm/AjaxVerifChat.php [L]
