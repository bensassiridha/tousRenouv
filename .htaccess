RewriteRule ^ttre_adm/chat.php$ ttre_adm/chat.php [L]
RewriteRule ^ttre_adm/index.php$ ttre_adm/index.php [L]


RewriteRule ^api_rest/espace_particulier.php$ api_rest/espace_particulier.php [L]



RewriteRule ^(.*)/$ zone/
RewriteRule ^(.*)/AjaxConnectAdmin.php$ zone/AjaxConnectAdmin.php
